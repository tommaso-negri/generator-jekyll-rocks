{
  "name": "generator-jekyll-rocks",
  "version": "0.1.1",
  "description":
    "Boost your Jekyll workflow with Grunt! Comes with cool features as livereload, sass, uglify, postcss and Git or FTP deploy. You can choose also your favourite front end framework and everything is automatic!",
  "homepage": "https://github.com/tommaso-negri/generator-jekyll-rocks",
  "author": {
    "name": "Tommaso Negri",
    "email": "tommaso.negri98@gmail.com",
    "url": "https://github.com/tommaso-negri"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "generator",
    "framework",
    "grunt",
    "bower",
    "jekyll",
    "livereload",
    "sass",
    "git",
    "ftp",
    "rocks",
    "bootstrap",
    "foundation",
    "semantic",
    "material-design-lite",
    "materialize"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "user": "0.0.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "tommaso-negri/generator-jekyll-rocks.git",
  "license": "MIT"
}
